{
  "name": "crypto-analytics",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:init": "tsx data/scripts/init-db.ts",
    "db:fetch": "tsx data/scripts/fetch-cryptocompare.ts",
    "db:fetch:quick": "tsx data/scripts/fetch-cryptocompare.ts --days=90",
    "db:fetch:year": "tsx data/scripts/fetch-cryptocompare.ts --days=365",
    "db:export": "tsx data/scripts/export-json.ts",
    "db:stats": "tsx data/scripts/db-stats.ts",
    "db:update": "npm run db:fetch:quick && npm run db:export",
    "db:futures": "tsx data/scripts/fetch-binance-futures.ts",
    "db:futures:5m": "tsx data/scripts/fetch-binance-futures.ts --interval=5m --days=365",
    "db:futures:15m": "tsx data/scripts/fetch-binance-futures.ts --interval=15m --days=90",
    "db:futures:4h": "tsx data/scripts/fetch-binance-futures.ts --interval=4h --days=90",
    "db:futures:1d": "tsx data/scripts/fetch-binance-futures.ts --interval=1d --days=365",
    "db:futures:all": "tsx data/scripts/fetch-binance-futures.ts --interval=15m --all",
    "db:futures:all-5m": "tsx data/scripts/fetch-binance-futures.ts --interval=5m --all",
    "db:futures:export": "tsx data/scripts/export-futures-json.ts",
    "db:futures:update": "npm run db:futures:all && npm run db:futures:export"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "duckdb-async": "^1.4.2",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.6.0",
    "tailwind-merge": "^3.4.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
